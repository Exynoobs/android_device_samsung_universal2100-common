service vendor.sensors-hal-multihal /system/bin/sh -c "sleep 2; /vendor/bin/hw/android.hardware.sensors-service.samsung-multihal.exynos2100"
    class hal
    user system
    group system wakelock context_hub input
    task_profiles ServiceCapacityLow
    capabilities BLOCK_SUSPEND
    rlimit rtprio 10 10
